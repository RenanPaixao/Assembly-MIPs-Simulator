{
  "compilerOptions": {
    "module": "ESNext",
    "target": "es5",
    "sourceMap": true,
    "strict": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "lib": ["ESNext", "dom"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noImplicitAny": false
  },
  "exclude": [
    "node_modules"
  ]
}